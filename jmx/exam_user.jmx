          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor" enabled="true">
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">import org.json.*;
import java.util.*;

String response_data = prev.getResponseDataAsString();
JSONObject data_obj = new JSONObject(response_data);
String parts_str = data_obj.get(&quot;paperPartList&quot;).toString();
JSONArray parts_array = new JSONArray(parts_str);
List results = new ArrayList();
String examRecId = vars.get(&quot;examRecId&quot;);

Random random = new Random();
for (int i = 0, n = parts_array.length(); i &lt; n; i++) {
  JSONObject part_obj = new JSONObject(parts_array.get(i).toString());
  String partId = part_obj.get(&quot;partId&quot;).toString();
  JSONArray quesList = new JSONArray(part_obj.get(&quot;partQusList&quot;).toString());

  for (int j = 0, jn = quesList.length(); j &lt; jn; j++) {
      JSONObject ques_obj = new JSONObject(quesList.get(j).toString());
      String qusId = ques_obj.get(&quot;questionId&quot;).toString();
      JSONObject ques_obj_detail = new JSONObject(ques_obj.get(&quot;questionDetail&quot;).toString());
      String subQuesStr = ques_obj_detail.get(&quot;subQuestions&quot;).toString();

      if (!&quot;null&quot;.equals(subQuesStr)) {
          JSONArray subQuesList = new JSONArray(subQuesStr);
          for (int k = 0, kn = subQuesList.length(); k &lt; kn; k++) {
              JSONObject sub_ques_obj = new JSONObject(subQuesList.get(k).toString());
              String subQuesId = sub_ques_obj.get(&quot;questionId&quot;).toString();
              String subAnsStr = sub_ques_obj.get(&quot;answers&quot;).toString();
              JSONArray subAnswersArray = new JSONArray(subAnsStr);
              int subRandomIndex = random.nextInt(subAnswersArray.length());
              JSONObject sub_ans_obj = new JSONObject(subAnswersArray.get(subRandomIndex).toString());
              JSONObject subNewJson = new JSONObject();
              subNewJson.put(&quot;examRecId&quot;, examRecId);
              subNewJson.put(&quot;partId&quot;, Integer.parseInt(partId));
              subNewJson.put(&quot;qusId&quot;, Integer.parseInt(subQuesId));
              subNewJson.put(&quot;answer&quot;, sub_ans_obj.get(&quot;answerId&quot;).toString());
              results.add(subNewJson.toString());
          }
      }

      String ansStr = ques_obj_detail.get(&quot;answers&quot;).toString();
      JSONArray answersArray = new JSONArray(ansStr);
      int randomIndex = random.nextInt(answersArray.length());
      JSONObject ans_obj = new JSONObject(answersArray.get(randomIndex).toString());
      JSONObject newJson = new JSONObject();
      newJson.put(&quot;examRecId&quot;, examRecId);
      newJson.put(&quot;partId&quot;, Integer.parseInt(partId));
      newJson.put(&quot;qusId&quot;, Integer.parseInt(qusId));
      newJson.put(&quot;answer&quot;, ans_obj.get(&quot;answerId&quot;).toString());
      results.add(newJson.toString());
  }
}
vars.put(&quot;answers&quot;,results.toString());</stringProp>
